	<script language="javascript">
function validateEmail(form_id,email) {
 
   var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
   var address = document.forms[form_id].elements[email].value;
   if(reg.test(address) == false) {
 	return "Invalid Email Address";
   
   
   }return "";
}
	function validate()
	{
	 check();
	var s =validateEmail("RegistrationForm","Email");
		var form = document.RegistrationForm;
		var msg = "";
		
		if( form.Name.value == "" )
		{
			msg += "     * Your name.\r\n";
		}
		if( form.Email.value == "" )
		{
			msg += "     * Your Email Address.\r\n";
		}else  if(s!="")
			{
				msg += "       "+s+"\r\n";
			 
			
			}
		if( form.Email1.value == "" || form.Name1.value == "" )
		{
			msg += "     * At least one of your friend's names and email addresses.\r\n";
		}else if(validateEmail("RegistrationForm","Email1")!="")
		{s=validateEmail("RegistrationForm","Email1");
				msg += "       "+s+"";
		}
		
		if( !msg == "" )
		{
			alert( "Please provide the following required information\r\n" + msg );
			return false;
		}
		else
		{
			return true;
		}
	}
	function check()
	{
	if( document.RegistrationForm.Email1.value != "" ||document.RegistrationForm.Name1.value != "" )
		{
		//alert("");
		}
	
