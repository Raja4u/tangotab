<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tangotab.dao.pojo.Events" table="events">
        <id name="eventId" type="java.lang.Integer">
            <column name="event_id" />
            <generator class="identity" />
        </id>
        <property name="eventName" type="java.lang.String">
            <column name="event_name" length="100" not-null="true" />
        </property>
        <property name="startDate" type="java.sql.Timestamp">
            <column name="start_date" length="19" />
        </property>
        <property name="endDate" type="java.sql.Timestamp">
            <column name="end_date" length="19" />
        </property>
        <property name="eventCode" type="java.lang.String">
            <column name="event_code" length="100" not-null="true" unique="true" />
        </property>
        <property name="isActive" type="java.lang.Short">
            <column name="is_active" not-null="true" />
        </property>
        <!--  <one-to-one name="consumerEvent" class="com.tangotab.dao.pojo.ConsumerEvent"></one-to-one>
   --> </class>
</hibernate-mapping>
