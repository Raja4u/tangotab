package com.tangotab.dao.pojo;

import java.util.HashSet;
import java.util.Set;

public class SalesManagerInfo implements java.io.Serializable {
	private static final long serialVersionUID = 1L;
	private Integer salesManagerId;
	private User user;
	private SalesInfo salesInfo;
	private Set<SalesMgrCities> salesMgrCitieses = new HashSet<SalesMgrCities>();
	private Set<SalesRepresentInfo> assignedRepresentatives = new HashSet<SalesRepresentInfo>();

	public Set<SalesRepresentInfo> getAssignedRepresentatives() {
		return assignedRepresentatives;
	}

	public void setAssignedRepresentatives(Set<SalesRepresentInfo> assignedRepresentatives) {
		this.assignedRepresentatives = assignedRepresentatives;
	}

	public Integer getSalesManagerId() {
		return salesManagerId;
	}

	public void setSalesManagerId(Integer salesManagerId) {
		this.salesManagerId = salesManagerId;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public SalesInfo getSalesInfo() {
		return salesInfo;
	}

	public void setSalesInfo(SalesInfo salesInfo) {
		this.salesInfo = salesInfo;
	}

	public Set<SalesMgrCities> getSalesMgrCitieses() {
		return salesMgrCitieses;
	}

	public void setSalesMgrCitieses(Set<SalesMgrCities> salesMgrCitieses) {
		this.salesMgrCitieses = salesMgrCitieses;
	}

}