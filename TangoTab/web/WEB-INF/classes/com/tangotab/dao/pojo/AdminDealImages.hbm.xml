<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tangotab.dao.pojo.AdminDealImages" table="admin_deal_images" >
        <id name="dealImagId" type="java.lang.Integer">
            <column name="DEAL_IMAG_ID" />
            <generator class="identity" />
        </id>
        <property name="dealImgPath" type="java.lang.String">
            <column name="DEAL_IMG_PATH" length="500" not-null="true" />
        </property>
           <many-to-one name="dealName" class="com.tangotab.dao.pojo.DealName" fetch="select" lazy="false">
            <column name="DEAL_NAME_ID" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>
