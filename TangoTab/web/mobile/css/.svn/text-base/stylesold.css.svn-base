/* CSS Document */

body{max-width:320; margin:0px; background-color:#999999; font-size: medium; font-family:calibri;-webkit-text-size-adjust: none;}
.cantainer{width:320px; height:auto; background-color:#FFFFFF;}
.text1{font-family:calibri; font-size:24px; font-style:normal; color:#000000; font-weight:bold;}
.text2{font-family:calibri; font-size:24px; font-style:normal; color:#FF6600; font-weight:bold;}
.padding1{padding-top:10px;}
.text_box{margin:0px;padding:4px;font-size:18px;width:90%;height:auto;border:1px solid #999999;-webkit-border-radius: 10px;-moz-border-radius: 10px;border-radius: 10px;}
.text3{font-family:calibri; font-size:20; font-style:normal; color:#000000; font-weight:normal;}
.text4{font-family:calibri; font-size:20; font-style:normal; color:#ff9838; font-weight:normal;}
.text4:hover{font-family:calibri; font-size:20; font-style:normal; color:#ff9838; font-weight:normal; text-decoration:underline;}
.line_img{background-image:url(../images/line_img.gif); background-repeat:repeat-x;}