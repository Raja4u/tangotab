	<script language="javascript">

	function validate()
	{
		var form = document.RegistrationForm;
		var msg = "";
		
		if( form.Name.value == "" )
		{
			msg += "     * Your name.\r\n";
		}
		if( form.Email.value == "" )
		{
			msg += "     * Your Email Address.\r\n";
		}
		if( form.Email1.value == "" || form.Name1.value == "" )
		{
			msg += "     * At least one of your friend's names and email addresses.\r\n";
		}
		
		if( !msg == "" )
		{
			alert( "Please provide the following required information\r\n" + msg );
			return false;
		}
		else
		{
			return true;
		}
	}
