<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tangotab.dao.pojo.Support" table="support">
        <id name="supportId" type="java.lang.Integer">
            <column name="SUPPORT_ID" />
            <generator class="identity" />
        </id>
        <property name="emailId" type="java.lang.String" >
            <column name="EMAIL_ID" length="40" />
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="STATUS" length="4"/>
        </property>
        <property name="roleId" type="java.lang.Integer">
            <column name="ROLE_ID" length="4" />
        </property>
        <property name="subject" type="java.lang.String">
            <column name="SUBJECT" length="30" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" length="500" />
        </property>
        <property name="supportDate" type="java.sql.Timestamp">
            <column name="SUPPORT_DATE" length="19" />
        </property>
    </class>
</hibernate-mapping>

