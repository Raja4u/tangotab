<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tangotab.dao.pojo.SalesInfo" table="sales_info">
        <id name="salesInfoId" type="java.lang.Integer">
            <column name="sales_info_id" />
            <generator class="identity" />
        </id>
        <property name="hireDate" type="java.util.Date">
            <column name="hire_date" length="10" />
        </property>
        <property name="terminationDate" type="java.util.Date">
            <column name="termination_date" length="10" />
        </property>
        <one-to-one name="salesRepresentInfos" class="com.tangotab.dao.pojo.SalesRepresentInfo" cascade="all" property-ref="salesInfo"/>
        <one-to-one name="salesManagerInfos" class="com.tangotab.dao.pojo.SalesManagerInfo" cascade="all" property-ref="salesInfo"/>
       <many-to-one name="city" class="com.tangotab.dao.pojo.City" fetch="select" lazy="proxy">
            <column name="city_id" />
        </many-to-one>
    </class>
</hibernate-mapping>
