<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tangotab.dao.pojo.ConsumerEvent" table="consumer_event">
        <id name="consumerEventId" type="java.lang.Integer">
            <column name="consumer_event_id" />
            <generator class="identity" />
        </id>
 
             
          <many-to-one name="events" class="com.tangotab.dao.pojo.Events" fetch="select" lazy="proxy">
            <column name="event_id" not-null="true" />
        </many-to-one>
 
           <many-to-one name="user" class="com.tangotab.dao.pojo.User" fetch="select"  lazy="proxy">
            <column name="USER_ID" />
        </many-to-one>
    </class>
</hibernate-mapping>
